            lv_data = iv_attr_value;&#13;
            lo_data = lv_data;&#13;
        }&#13;
&#13;
        fx_trace(SCRIPT+"Returning "+lo_data);&#13;
        return lo_data;&#13;
&#13;
    }// parseAttributeValue&#13;
