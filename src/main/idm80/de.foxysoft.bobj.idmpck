&#13;
    function fx_generatePolicyPasswordImpl()&#13;
    {&#13;
        fx_trace(SCRIPT+"Entering");&#13;
        var lv_result;&#13;
&#13;
        try&#13;
        {&#13;
&#13;
            if(!gv_initialized)&#13;
            {&#13;
                init();&#13;
            }&#13;
&#13;
